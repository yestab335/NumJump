{
  "name": "NumJump",
  "version": "1.0.0",
  "description": "A simple extension that prefixes numbers based on a tab's position",
  "author": "Adam Ben-Aamr",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
